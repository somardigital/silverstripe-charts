{
    "name": "flashbackzoo/silverstripe-charts",
    "type": "silverstripe-vendormodule",
    "description": "Display your data using Chart.js",
    "license": "MIT",
    "keywords": ["SilverStripe", "Chart.js", "Graph"],
    "authors": [
        {
            "name": "David Craig",
            "email": "david@davidcraig.co.nz"
        },
        {
            "name": "Ian Herbert",
            "email": "ian@silverstripe.com"
        },
        {
            "name": "Jared Lee",
            "email": "jared@silverstripe.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "silverstripe/framework": "^5",
        "silverstripe/cms": "^5",
        "undefinedoffset/sortablegridfield": "^2",
        "tractorcow/silverstripe-colorpicker": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
